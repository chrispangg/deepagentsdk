{
  "name": "ai-sdk-deep-agent",
  "version": "0.9.1",
  "description": "Deep Agents implementation using Vercel AI SDK - build controllable AI agents with planning, filesystem, and subagent capabilities",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "bin": {
    "deep-agent": "./src/cli/index.tsx"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./cli": {
      "import": "./src/cli/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun test test/",
    "test:integration": "bun test test-integration/",
    "test:all": "bun run test && bun run test:integration",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck",
    "cli": "bun src/cli/index.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrispangg/ai-sdk-deepagent.git"
  },
  "homepage": "https://github.com/chrispangg/ai-sdk-deepagent#readme",
  "bugs": {
    "url": "https://github.com/chrispangg/ai-sdk-deepagent/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0-beta.83",
    "@ai-sdk/openai": "^3.0.0-beta.96",
    "@ai-sdk/react": "^3.0.0-beta.153",
    "@inkjs/ui": "^1.0.0",
    "@mozilla/readability": "^0.6.0",
    "@tavily/core": "^0.6.1",
    "ai": "^6.0.0-beta.150",
    "fast-glob": "^3.3.3",
    "ink": "^5.1.0",
    "jsdom": "^25.0.1",
    "micromatch": "^4.0.8",
    "react": "^18.2.0",
    "react-devtools-core": "^5.3.2",
    "turndown": "^7.2.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@langfuse/otel": "^4.5.1",
    "@langfuse/tracing": "^4.5.1",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@types/micromatch": "^4.0.9",
    "@types/react": "^18.3.18",
    "@types/turndown": "^5.0.5",
    "@vercel/otel": "^2.1.0",
    "install": "^0.13.0",
    "langfuse-vercel": "^3.38.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "ai",
    "agents",
    "deep-agents",
    "vercel-ai-sdk",
    "llm",
    "typescript",
    "bun",
    "anthropic",
    "openai",
    "ai-sdk"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "license": "MIT"
}
